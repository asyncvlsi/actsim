/*

    Test basic functionality of file source with logging on and looping off
    for reading hex values

*/

import sim;
import globals;

defproc test ()
{
    pint F_ID = 0;

    sim::source_file_en<8, F_ID, false, 0, true> src;
    sim::sink_en<8, 0, true> snk;

    snk.I = src.O;

    snk.enable = Vdd;
    src.enable = Vdd;
}
